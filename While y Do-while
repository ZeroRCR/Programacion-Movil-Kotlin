fun main (){
    var contador = 0
    do {
        println("Contador: $contador")
        contador++
    } while (contador < 5)
}

var numero = 1
var suma = 0

while (numero <= 10) {
    if (numero % 2 == 0) {
        suma += numero
    }
    numero++
}

println("La suma de nÃºmeros pares del 1 al 10 es: $suma")
